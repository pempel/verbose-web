{
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "node": true
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "indent": [2, 2],
    "no-console": 0,
    "no-unused-vars": 0,
    "react/jsx-uses-react": 1,
    "semi": [2, "always"],
  }
}
